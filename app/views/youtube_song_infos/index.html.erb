<p id="notice"><%= notice %></p>

<h1>Listing Youtube Song Infos</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Youtube</th>
      <th>Time</th>
      <th>Attr</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @youtube_song_infos.each do |youtube_song_info| %>
      <tr>
        <td><%= youtube_song_info.title %></td>
        <td><%= youtube_song_info.youtube_id %></td>
        <td><%= youtube_song_info.time %></td>
        <td><%= youtube_song_info.attr %></td>
        <td><%= link_to 'Show', youtube_song_info %></td>
        <td><%= link_to 'Edit', edit_youtube_song_info_path(youtube_song_info) %></td>
        <td><%= link_to 'Destroy', youtube_song_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Youtube song info', new_youtube_song_info_path %>
